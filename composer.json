{
  "name": "geeh/sevenificator",
  "description": "PHP 7 strict type hinting converter (for PHP 7)",
  "minimum-stability": "dev",
  "license": "MIT",
  "authors": [
    {
      "name": "Gary @GeeH Hockin",
      "email": "gary@roave.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "GeeH\\Sevenificator\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "GeeH\\SevenificatorTest\\": "test/"
    }
  },
  "require": {
    "php": "~7",
    "phpdocumentor/phpdocumentor": "~2.8.3",
    "monolog/monolog": "~1.6",
    "asgrim/better-reflection": "dev-master"
  },
  "require-dev": {
    "zendframework/zend-eventmanager": "~2.5",
    "symfony/console": "~2.1"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:asgrim/better-reflection.git"
    }
  ],
  "bin": ["bin/sevenificate"]
}
